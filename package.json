{
  "name": "marketplace-partners-api",
  "version": "0.0.1",
  "private": false,
  "type": "commonjs",
  "engines": {
    "node": ">=16.15.0"
  },
  "license": "Apache-2.0",
  "scripts": {
    "server": "node server.js",
    "app": "node app.js",
