    "categories:refresh:ai": "node \"services/makeCategories\" start",
    "images:products:fix": "node \"services/imageFix\" products",
    "backup:s3": "node \"services/backupS3\"",
    "backup:mongo:api": "node --max-old-space-size=4096 \"services/backupMongo\" api",
    "backup:mongo:partners_api": "node --max-old-space-size=8000 \"services/backupMongo\" partners_api"
