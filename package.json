    "stores:check": "node services/storesChecker",
    "stores:update:logo": "node \"services/storesUpdate\" logo",
    "stores:scrape:logo": "node \"services/scrapeStoresLogo\" start",
    "stores:update:commission": "node \"services/storesUpdate\" commission",
    "stores:update:categories": "node \"services/storesUpdate\" categories",
    "stores:update:description": "node \"services/storesUpdate\" description",
    "stores:update:country": "node \"services/storesUpdate\" country",
    "stores:update:country-code": "node \"services/storesUpdate\" country-code",
