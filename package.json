    "csv-parser": "^3.0.0",
    "dotenv": "^8.2.0",
    "ebay-oauth-nodejs-client": "^1.2.1",
    "es6-promisify": "^6.1.1",
    "express": "^4.18.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-validator": "^6.14.2",
    "fs": "^0.0.1-security",
    "googleapis": "^109.0.1",
    "helmet": "^6.0.1",
    "i18n": "^0.15.1",
    "moment": "^2.29.1",
    "mongoose": "^6.0.2",
    "node-fetch": "2",
    "nyc": "^15.1.0",
    "object-hash": "^3.0.0",
    "open-exchange-rates": "^0.3.0",
    "p4u-client-ai": "^0.0.8",
    "puppeteer": "^19.4.0",
    "redis": "^3.1.2",
    "request": "^2.88.2",
    "rollbar": "^2.21.0",
    "short-unique-id": "^4.4.4",
    "systeminformation": "^5.21.24",
    "tld-countries": "^1.0.1",
    "tslib": "^2.1.0",
    "winston": "^3.8.2",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "chai": "^4.3.4",
    "chai-http": "^4.3.0",
    "eslint": "^7.22.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "husky": "5.1.3",
    "lint-staged": "^10.5.3",
    "mocha": "^5.1.1",
    "pm2": "^5.2.2",
    "prettier": "^2.2.1"
  },
  "lint-staged": {
    "**/*.js": [
      "eslint --cache --fix",
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
