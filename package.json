    "cron:test": "node \"services/cronMaintenanceSchedule.js\" test",
    "cron:repeated": "node \"services/repeatedMongo.js\" repeated",
    "db:replication": "node \"services/mongoReplication.js\"",
    "seed": "node \"database/seeds/index.js\"",
    "test": "mocha --timeout 1200000 --exit",
